msgctxt "Dialogue_Response,DL_Grinch2_PlayerResponse_1"
msgid "Goodbye?"
msgstr "Au revoir ?"

#. Key:	DL_Grinch_PlayerResponse_1
#. SourceLocation:	/Game/BluePrints/Classes/Dialogue/Lines/Holidays/DL_Grinch.Default__DL_Grinch_C.Responses(0).Responses.ResponseText_20_B7867F0F4B34A10C5D626B84DD15AB04
