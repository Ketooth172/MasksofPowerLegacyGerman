msgctxt "Quest_Objective,Quest_TheTuragaOfWater_Objective_2"
msgid "Defeat the Tarakava."
msgstr "Vainquez le Tarakava."

#. Key:	Quest_TheTuragaOfWater_Objective_3
#. SourceLocation:	/Game/BluePrints/Classes/Quests/Main/Quest_TheTuragaOfWater.Default__Quest_TheTuragaOfWater_C.Objectives(2).Objectives.ObjectiveDescription_47_E55EE7E84E5B1D4A152D00844ADD816F
